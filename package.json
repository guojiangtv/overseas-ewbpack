    "vn_deploy": "cross-env-shell NODE_ENV=production COUNTRY=vietnam node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --colors --config ./webpack.config.js",
    "arb_deploy": "cross-env-shell NODE_ENV=production COUNTRY=arb node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --colors --config ./webpack.config.js",
    "pc_dll": "cross-env-shell PLATFORM=pc NODE_ENV=production node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --progress --colors --config ./webpack-dll.config.js",
