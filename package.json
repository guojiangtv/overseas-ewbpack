    "pc_build": "cross-env-shell PLATFORM=pc node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --hide-modules",
    "pc_deploy": "cross-env-shell PLATFORM=pc NODE_ENV=production webpack --bail --progress --colors --config ./webpack.config.js",
    "new": "node node_modules/yo/lib/cli.js ./generators"
