    "vn_build": "cross-env-shell COUNTRY=vietnam node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --hide-modules",
    "arb_build": "cross-env-shell COUNTRY=arb node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --hide-modules",
